  },
  "homepage": "https://tomkonidas.com",
  "devDependencies": {
    "@11ty/eleventy": "0.11.1",
    "alpinejs": "2.8.0",
    "autoprefixer": "10.2.4",
    "clean-css-cli": "5.2.0",
    "html-minifier": "4.0.0",
    "node-fetch": "2.6.1",
    "postcss-cli": "8.3.1",
    "tailwindcss": "2.0.3"
